{
  "name": "react-ultimate-pagination",
  "version": "1.0.2",
  "description": "React.js pagination component based on ultimate-pagination",
  "main": "lib/react-ultimate-pagination.js",
  "keywords": [
    "react",
    "react.js",
    "component",
    "react-component",
    "pagination",
    "paginator",
    "pager"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prebuild": "rm -rf dist lib",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel src -d lib",
    "build:umd": "webpack --output-filename react-ultimate-pagination.js",
    "build:umd.min": "webpack --output-filename react-ultimate-pagination.min.js -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ultimate-pagination/react-ultimate-pagination.git"
  },
  "author": "Dmytro Yarmak <dmytroyarmak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ultimate-pagination/react-ultimate-pagination/issues"
  },
  "homepage": "https://github.com/ultimate-pagination/react-ultimate-pagination",
  "dependencies": {
    "prop-types": "^15.0.0",
    "ultimate-pagination": "1.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "npm-run-all": "^4.0.1",
    "webpack": "^2.2.1"
  }
}
